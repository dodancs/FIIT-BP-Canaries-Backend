    "version": "0.2.0",
    "configurations": [
        {
            "name": "Backend: run",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\canaries.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Backend: help",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\canaries.py",
            "args": ["-h"],
            "console": "integratedTerminal"
        },
        {
            "name": "Backend: setup",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\canaries.py",
            "args": ["--setup"],
            "console": "integratedTerminal"
        },
        {
            "name": "Backend: bad arguments",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\canaries.py",
            "args": ["-c a.ini"],
            "console": "integratedTerminal"
        }
    ]
}